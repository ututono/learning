package 实验二;

import java.awt.Point;

public class Circle {
	private static double x,y,radius;
	public Circle(double x,double y){
		this.x=x;
		this.y=y;
		this.radius=1;
	}
	public double getX(){
		return x;
	}
	public void setX(double x){
		this.x=x;
	}
	public double getY(){
		return y;
	}
	public void setY(double y){
		this.y=y;
	}
	public double getradius(){
		return radius;
	}
	public void setradius(double radius){
		this.radius=radius;
	}
	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		Circle circle1=new Circle(2.0,5.0);
		System.out.println("初始化:\n"+"圆的x轴坐标："+circle1.getX()+"\n圆的y轴坐标："+circle1.getY());
		
	}

}
